<div class="contents row">
  <h2>書き直し</h2>
  <%= form_with(url: "/blogs", method: :post) do |f|%>

   <div class="field">
   	 <%= f.label "タイトル" %>
   	 <%= f.text_field :title %>
   </div>

   <div class="field">
   	 <%= f.label "記事" %>
   	 <%= f.text_area(:text, cols: 300, rows: 40) %>
   </div>

  <%= f.submit "送信"%>
  <% end %>
</div>